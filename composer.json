{
    "require": {
        "php": ">=7.3",
        "silverstripe/recipe-cms": "^4",
        "silverstripe-themes/simple": "~3.2.0",
        "bigfork/silverstripe-oauth-login": "^2.0",
        "guzzlehttp/guzzle": "~6.0",
        "sminnee/tagmanager": "^1.0",
        "dnadesign/silverstripe-elemental-userforms": "^3.0",
        "silverstripe/bambusa-theme": "dev-master",
        "revstrat/silverstripe-oauth-buttons": "^0.6.1",
        "league/omnipay": "^3.0",
        "silverstripe/silverstripe-omnipay": "dev-master",
        "omnipay/stripe": "^3.1",
        "omnipay/paypal": "^3.0",
        "omnipay/manual": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8",
        "squizlabs/php_codesniffer": "^3.4"
    },
    "extra": {
        "resources-dir": "_resources",
        "project-files-installed": [
            "app/.htaccess",
            "app/_config.php",
            "app/_config/mysite.yml",
            "app/src/Page.php",
            "app/src/PageController.php"
        ],
        "public-files-installed": [
            ".htaccess",
            "index.php",
            "install-frameworkmissing.html",
            "install.php",
            "web.config"
        ]
    },
    "autoload": {
        "files": ["app/src/bootstrap.php"],
        "psr-4": {
            "App\\": "app/src"
        }
    },
    "config": {
        "process-timeout": 600
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
